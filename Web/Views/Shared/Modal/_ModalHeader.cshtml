@using Global.Abstractions.ViewModels

@model IModalVm

<div class="modal-header santa-modal-header align-items-center justify-content-center">
    <h4 class="modal-title text-center w-100">@Model.ModalTitle</h4>
    <div class="d-inline" data-toggle="tooltip" data-html="true" data-bs-html="true" data-bs-container="label" title="Close this dialog">
        <button type="button" class="btn btn-close-override" data-bs-dismiss="modal" aria-label="Close"><i class="fa fa-close"></i></button>
    </div>
</div>