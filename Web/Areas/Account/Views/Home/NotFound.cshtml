@model string?
@{
    ViewData["Title"] = "Not Found";
}

<h1>Access Denied</h1>
<section>
    <div class="row @CenterFlexNoWrapContainer">
        <div class="mb-5">
            @if (Model.IsNotEmpty())
            {
                @Model
            }
            else
            {
                <text>The requested item could not be found; you may not have access to it. Please go back and try again.</text>
            }
        </div>
    </div>
</section>