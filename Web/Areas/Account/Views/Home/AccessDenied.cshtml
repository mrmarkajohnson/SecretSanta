@model string?
@{
    ViewData["Title"] = "Access Denied";
}

<h1>Access Denied</h1>
<section>
    <div class="row @CenterFlexNoWrapContainer">
        <div class="mb-5">
            You do not have access to the requested page.

            @if (Model.IsNotEmpty())
            {
                @Model
            }
        </div>
    </div>
</section>