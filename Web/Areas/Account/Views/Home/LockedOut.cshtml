@using Global.Validation

@{
    ViewData["Title"] = "Locked Out";
}

<h1>This account is locked out</h1>
<section>
    <div class="row d-flex align-items-center justify-content-center text-center">
        <div class="mb-5">                    
            The requested account has been locked out for @IdentityVal.Lockouts.DefaultLockoutTimeSpan.GetDescription().                    
        </div>
    </div>
</section>